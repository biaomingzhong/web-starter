{
  "name": "react-vite-admin",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "serve": "pnpm dev",
    "dev": "vite --open --config ./vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build --config ./vite.config.prod.ts",
    "preview": "vite preview --open",
    "prettier": "npx prettier --write .",
    "analysis": "windicss-analysis",
    "analysis-build": "windicss-analysis --html dist",
    "lint": "eslint ./src --ext .js,.ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .js,.ts,.tsx",
    "coverage": "vitest run --coverage",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@ebay/nice-modal-react": "^1.2.1",
    "@loadable/component": "^5.15.2",
    "@rematch/core": "^2.2.0",
    "@rematch/loading": "^2.1.2",
    "ahooks": "^3.1.13",
    "antd": "^4.19.2",
    "antd-form-builder": "^2.1.4",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.0",
    "hoist-non-react-statics": "^3.3.2",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "store2": "^2.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/toastify-js": "^1.11.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-react": "^1.2.0",
    "@vitest/ui": "latest",
    "faker": "^5.5.3",
    "jsdom": "latest",
    "miragejs": "^0.1.43",
    "react-use": "^17.3.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.44.0",
    "typescript": "^4.5.4",
    "vite": "latest",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-windicss": "^1.6.3",
    "vite-svg-loader": "^3.1.2",
    "vitest": "latest",
    "vue-tsc": "^0.33.2-patch.1",
    "windicss": "^3.5.1",
    "windicss-analysis": "^0.3.5"
  }
}
