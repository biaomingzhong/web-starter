{
  "name": "react-vite-admin",
  "version": "0.1.0",
  "scripts": {
    "init": "yarn install",
    "dev": "vite --mode development",
    "build": "tsc --noEmit && vite build --mode production",
    "preview": "vite preview --port 4000",
    "prettier": "npx prettier --write .",
    "lint": "eslint ./src --ext .js,.ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .js,.ts,.tsx",
    "analysis": "windicss-analysis",
    "analysis-build": "windicss-analysis --html dist",
    "msw:update": "npx msw init public --no-save"
  },
  "dependencies": {
    "ahooks": "^3.1.1",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.21",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@mswjs/data": "^0.8.3",
    "@types/faker": "^5.5.9",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-legacy": "^1.6.3",
    "@vitejs/plugin-react": "^1.0.0",
    "faker": "^5.5.3",
    "jwt-decode": "^3.1.2",
    "msw": "^0.36.0",
    "nanoid": "^3.1.30",
    "prettier": "^2.4.1",
    "sass": "^1.44.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-windicss": "^1.5.3",
    "windicss": "^3.2.1",
    "windicss-analysis": "^0.3.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
