{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "Taro" }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx"] }],
    
    "import/prefer-default-export": 0,
    "semi-spacing": [2, {"before": false,"after": true}]
  },
    "parser": "babel-eslint"
  }
