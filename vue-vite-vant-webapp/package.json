{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "author": "moocss <moocss@gmail.com>",
  "scripts": {
    "serve": "pnpm dev",
    "dev:mock": "vite --open --config ./vite.config.mock.ts",
    "dev": "vite --open --config ./vite.config.dev.ts",
    "build:staging": "vue-tsc --noEmit && vite build --config ./vite.config.staging.ts",
    "build": "vue-tsc --noEmit && vite build --config ./vite.config.prod.ts",
    "preview": "vite preview --open",
    "prettier": "npx prettier --write .",
    "lint": "eslint ./src --ext .vue,.js,.ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts,.tsx",
    "lint-staged": "npx lint-staged",
    "coverage": "vitest --coverage",
    "test": "vitest"
  },

  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "echarts": "^5.2.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.9",
    "pinia-plugin-persistedstate": "^1.0.3",
    "register-service-worker": "^1.7.2",
    "store2": "^2.12.0",
    "vant": "^3.4.2",
    "vue": "^3.2.16",
    "vue-echarts": "^6.0.0",
    "vue-request": "^1.2.3",
    "vue-i18n": "^9.2.0-beta.28",
    "vue-router": "^4.0.11",
    "toastify-js": "^1.11.2"
  },
  "devDependencies": {
    "@types/echarts": "^4.9.12",
    "@types/lodash": "^4.14.178",
    "@types/mockjs": "^1.0.6",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^17.0.8",
    "@types/nprogress": "^0.2.0",
    "@types/toastify-js": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "happy-dom": "latest",
    "less": "^4.1.2",
    "lint-staged": "^12.2.2",
    "mockjs": "^1.1.0",
    "nanoid": "^3.1.30",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-px2rem-exclude": "^0.0.6",
    "prettier": "^2.4.1",
    "sass": "^1.44.0",
    "typescript": "^4.4.3",
    "vite": "^2.7.13",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^1.4.1",
    "vite-svg-loader": "^3.1.1",
    "vitest": "latest",
    "vue-tsc": "^0.30.6",
    "rollup-plugin-visualizer": "^5.5.4"
  },
  "license": "MIT"
}
